{
  "label": "$SERVICE_NAME_$SERVICE_VERSION_$ARCH",
  "description": "$SERVICE_NAME for $ARCH",
  "url": "$SERVICE_NAME",
  "version": "$SERVICE_VERSION",
  "arch": "$ARCH",
  "public": true,
  "sharable": "singleton",
  "requiredServices": [],
  "userInput": [],
  "deployment": {
    "services": {
      "$SERVICE_NAME": {
        "image": "$SERVICE_CONTAINER",
        "binds": ["$MMS_SHARED_VOLUME:$VOLUME_MOUNT:rw"],
        "ports": [
          { "HostIP": "0.0.0.0", "HostPort": "3000:3000/tcp" }
        ],
        "privileged": true
      }
    }
  },
  "deploymentSignature": "",
  "clusterDeployment": "",
  "clusterDeploymentSignature": "",
  "imageStore": {
    "storeType": "dockerRegistry"
  }
}
